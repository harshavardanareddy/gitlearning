pipeline{
	agent any
	stages{
			stage('Build'){
				steps{
					echo 'Building the project...'
					}
				}
				stage('Test'){
						steps{
							Input( 'u want to proceed testing the project...')
						}
				}
				stage('Deploy'){
						when{
							branch 'master'
						}
						steps{
							echo 'Deploying the project...'
						}
				}
	}
}